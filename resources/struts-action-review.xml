<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="review" namespace="/review" extends="struts-common">
	    <action name="getReviewForm" method="getReviewForm" class="ReviewAction">
	    	<result name="success">reviewForm.jsp</result>
	    </action>
	    <action name="saveReview" method="saveReview" class="ReviewAction">
	    	<result type="customJSON" name="success">jsonModel</result>
	    </action>
    </package>
</struts>
