<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="BidAction" class="nju.edu.auctionExp.action.participant.BidAction" scope="prototype">
		<property name="participantManager" ref="ParticipantManager"></property>
		<property name="auctionManager" ref="AuctionManager"></property>
		<property name="bidManager" ref="AuctionBidManager"></property>
	</bean>
</beans>